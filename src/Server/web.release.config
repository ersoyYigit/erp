		<cors enabled="true">
			<add origin="https://*.ardaglassware.com" allowCredentials="true" maxAge="120">
				<allowHeaders allowAllRequestedHeaders="true">
					<add header="Content-Type" />
					<add header="Authorization" />
				</allowHeaders>
				<allowMethods>
					<add method="DELETE" />
					<add method="GET" />
					<add method="POST" />
					<add method="PUT" />
					<add method="OPTIONS" />
				</allowMethods>
				<exposeHeaders>
					<add header="Content-Type" />
					<add header="Authorization" />
				</exposeHeaders>
			</add>
			<add origin="https://*.ardaglassware.com:44398" allowCredentials="true" maxAge="120">
				<allowHeaders allowAllRequestedHeaders="true">
					<add header="Content-Type" />
					<add header="Authorization" />
				</allowHeaders>
				<allowMethods>
					<add method="DELETE" />
					<add method="GET" />
					<add method="POST" />
					<add method="PUT" />
					<add method="OPTIONS" />
				</allowMethods>
				<exposeHeaders>
					<add header="Content-Type" />
					<add header="Authorization" />
				</exposeHeaders>
			</add>
		</cors>
		<handlers>
			<add name="aspNetCore" path="*" verb="*" modules="AspNetCoreModuleV2" resourceType="Unspecified" />
		</handlers>
		<aspNetCore processPath="dotnet" arguments=".\ArdaManager.Server.dll" stdoutLogEnabled="true" stdoutLogFile="\\?\%home%\LogFiles\stdout" hostingModel="inprocess" />
	</system.webServer>
