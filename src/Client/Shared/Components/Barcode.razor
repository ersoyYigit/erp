
<MudDialog>

    <DialogContent>
        <GenerateQRCode StringToEncode="@QrCodeText" />
    </DialogContent>
    <DialogActions>
        <MudButton OnClick="Submit">Kapat</MudButton>
    </DialogActions>
</MudDialog>

@code {
    [CascadingParameter] MudDialogInstance MudDialog { get; set; }
    [Parameter] public string QrCodeText { get; set; }


    void Submit() => MudDialog.Close(DialogResult.Ok(true));
    void Cancel() => MudDialog.Cancel();

}
